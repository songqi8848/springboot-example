<mapper namespace="com.zyy.mapper.StudentMapper">
    <insert id="insert" parameterType="com.zyy.entity.Student">
        insert into student_info(name,sex,phone) values(#{student.name},#{student.sex},#{student.phone})
    </insert>
</mapper>